<p id="notice"><%= notice %></p>

<h1>Bid Managements</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bid_managements.each do |bid_management| %>
      <tr>
        <td><%= bid_management.name %></td>
        <td><%= link_to 'Show', bid_management %></td>
        <td><%= link_to 'Edit', edit_bid_management_path(bid_management) %></td>
        <td><%= link_to 'Destroy', bid_management, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bid Management', new_bid_management_path %>
